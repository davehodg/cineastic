<h1>[% actors.0.movie_actors.0.actor.title %]</h1>

[% USE Dumper(Pad="<br>") %]
[%# Dumper.dump(actors) %]

<ul>
[% FOREACH g =  actors %]
[%# Dumper.dump(g) %]

<li>
<!-- <img align="left" width="50" src="[% g.rt_img_thu %]"> -->
<a href="/site/movie/[% g.id%]/[% g.title | replace('\s+', '-')| replace(',+', '-') | replace('\.', '-') |url %]">[% g.title %] ([% g.year %])</a> ([% g.rt_critic %])
[% END %]
</ul>

<p>
[
[% IF page > 0 %]
<a href="[% c.req.uri.path %]?page=[% page - 1 %]">Prev</a> |
[% END %]
<a href="[% c.req.uri.path %]?page=[% page + 1 %]">Next</a>
]
</p>
